						</div>

						<div class="col-6">
							<p class="lead mb-1 mt-3">
							Type de cession
							</p>
							{{ form_widget(formAnnonce.typeCession) }}
						</div>

						<div class="col-6">
							<p class="lead mb-1 mt-3">
