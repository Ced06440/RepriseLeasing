			{{ form_label(formAnnonce.submit) }}

    {{ form_end(formAnnonce) }}
	
	{% else %}

	<div class="text-center">
		<h2>Pour déposer une annonce merci de vous inscrire avant</h2>
		<div class="d-none d-lg-block me-5 mb-5">
            <a href="{{ path('app_register') }}">
				<button class="btn btn-warning mt-3 btn-lg">
